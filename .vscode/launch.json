{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run sesmassuploadapp",
      "runtimeExecutable": "cds",
      "request": "launch",
      "args": [
        "watch",
        "--open"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "preLaunchTask": "PreLaunchTask for Run sesmassuploadapp",
      "envFile": "${workspaceFolder}/env/.env1",
      "env": {
        "run.config": "{\"handlerId\":\"lcap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/sesmassuploadapp\\\",\\\"profileName\\\":\\\"\\\"}\"}",
        "runConfigurations": "{\"odataBinding\":{\"ZAPI_PURCHASEORDER_PROCESS_SRV\":{\"destination\":\"L5S\",\"type\":\"Destination\",\"path\":\"/sap/opu/odata/sap/API_PURCHASEORDER_PROCESS_SRV\"},\"ZAPI_PURCHASECONTRACT_PROCESS_SRV\":{\"destination\":\"L5S\",\"type\":\"Destination\",\"path\":\"/sap/opu/odata/sap/API_PURCHASECONTRACT_PROCESS_SRV\"}}}"
      }
    }
  ]
}